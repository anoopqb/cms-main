{
  "kind": "singleType",
  "collectionName": "globals",
  "info": {
    "singularName": "global",
    "pluralName": "globals",
    "displayName": "Global"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "propertyName": {
      "type": "string"
    },
    "logo": {
      "type": "media",
      "multiple": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "phoneNumber": {
      "type": "biginteger"
    },
    "address": {
      "type": "text"
    },
    "primaryColor": {
      "type": "customField",
      "customField": "plugin::color-picker.color",
      "regex": "^#([A-Fa-f0-9]{6}|[A-Fa-f0-9]{3})$"
    },
    "topNavItems": {
      "type": "component",
      "component": "shared.cta",
      "repeatable": true
    },
    "officeHours": {
      "type": "component",
      "component": "shared.office-hours",
      "repeatable": true
    },
    "specialsText": {
      "type": "text"
    }
  }
}
